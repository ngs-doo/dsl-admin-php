<!DOCTYPE html>
<html lang="en">
<head>
    <title>PhpDslAdmin</title>
    <link rel="stylesheet" href="/static/css/bootstrap.min.css"/>
    
    <link href="/static/css/bootstrap-datetimepicker.css" type="text/css" rel="stylesheet" media="screen" />
    <link href="/static/css/php-ui.css" type="text/css" rel="stylesheet" media="screen" />
    
    <script type="text/javascript" src="/static/js/jquery-1.9.1.min.js"></script>
    <script type="text/javascript" src="/static/js/bootstrap.min.js"></script>
    <script type="text/javascript" src="/static/js/ngs.collection.js"></script>
    <script type="text/javascript" src="/static/js/ngs.grid.js"></script>
    <script type="text/javascript" src="/static/js/ngs.init.js"></script>
    <script type="text/javascript" src="/static/js/bootstrap-datetimepicker.js"></script>

    <link rel="shortcut icon" href="/favicon.ico">
    
    <style type="text/css">
        fieldset fieldset {
            margin-left: 40px;
        }
        form legend {
            font-size: 16px;
        }
        .btn-remove {
            float:right;
        }
        .lookup-control {
            width: 80%;
        }
    </style>
</head>
<body>
    <div class="row">
        <div class="col-lg-12">
            <ul class="nav nav-pills">
                <!--li class="dropdown">
                    <a data-toggle="dropdown" class="dropdown-toggle" href="#" title="Open project"><span class="glyphicon glyphicon-folder-open"></span></a>
                    <ul class="dropdown-menu">
                        {% if projects is defined %}
                        {% for project in projects %}
                            <li><a href="/{{ project }}">{{ project }}</a></li>
                        {% endfor %}
                        {% endif %}
                    </ul>
                </li-->
{% block navigation_links %}
                {% embed 'navig.twig' %}{% endembed %}
{% endblock navigation_links %}
            </ul>
        </div>
        <div class="col-lg-12">
{% block messages %}
            <div class="row">
                <ul class="col-lg-10 col-lg-offset-1 col-sm-10 col-sm-offset-1 messages list-unstyled">
{%          if app is defined and app.session is defined %}
{%              for msg in app.session.getFlashBag.get('message') %}
                    <li class="alert alert-{{ msg.type }}">{{ msg.content }}</li>
{%              endfor %}
{%          endif %}
                </ul>
            </div>
{% endblock messages %}
            {% block content %}{% endblock %}
        </div>

        <div id="mod" class="modal fade" tabindex="-1">
            <div class="modal-dialog modal-lg">
                <div class="modal-content">
                    <h1>modal</h1>
                </div>
            </div>
        </div>
    </div>
</body>
</html>
